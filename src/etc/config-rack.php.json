{
    "portal"     : {
        "url"     : "http://rack.php",
        "origin"  : "*",
        "session" : "native",
        "locale"  : "en_GB",
        "tempPath": "/var/www/hosting.dev/rack/tmp"
    },
    "storage"    : {
        "default": "file:path=/var/www/hosting.dev/rack/storage",
        "redis"  : "redis:host=localhost;port=6379;dbindex=0;ttl=3600"
    },
    "database"   : {
        "default": "pgsql:host=rack.php;port=5432;dbname=rack;user=rack;password=manager",
        "reserve": "mysql:host=rack.php;port=3306;dbname=rack;user=rack;password=manager;charset=utf8"
    },
    "logger"     : {
        "default": "file:level=DEBUG;path=/var/www/hosting.dev/rack/logs;prefix=rack"
    },
    "templates"  : {
        "engine"       : "smarty",
        "cacheDir"     : "/var/www/hosting.dev/rack/tmp/cache",
        "compileDir"   : "/var/www/hosting.dev/rack/tmp/compile",
        "enableCaching": "false",
        "forceCompile" : "false"
    },
    "notificator": {
        "url"   : "https://xns.xeaf.net",
        "key"   : "00000000-0000-0000-0000-000000000000",
        "enable": false
    }
}
