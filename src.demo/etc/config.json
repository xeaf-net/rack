{
    "portal"     : {
        "url"      : "https://rack.xeaf.space",
        "origin"   : "*",
        "bearer"   : "rackAPI",
        "locale"   : "en_GB",
        "session"  : "native",
        "tempPath" : "/tmp",
        "debugMode": "true"
    },
    "storage"    : {
        "default": "file:path=/var/www/hosting.space/rack/tmp",
        "redis"  : "redis:host=localhost;port=6379;dbindex=0;ttl=3600"
    },
    "database"   : {
        "default": "pgsql:host=rack.xeaf.space;port=5432;dbname=rack;user=rack;password=manager"
    },
    "mailer"     : {
        "smtp"      : true,
        "host"      : "localhost",
        "port"      : 25,
        "auth"      : false,
        "secure"    : "ssl",
        "userName"  : "",
        "password"  : "",
        "sendFrom"  : "noreplay@example.com",
        "senderName": "rackAPI",
        "charSet"   : "utf-8"
    },
    "logger"     : {
        "default": "file:level=DEBUG;path=/var/www/hosting.space/rack/logs;prefix=rack"
    },
    "templates"  : {
        "engine"       : "smarty",
        "cacheDir"     : "/var/www/hosting.space/rack/tmp/smarty",
        "compileDir"   : "/var/www/hosting.space/rack/tmp/smarty",
        "enableCaching": "false",
        "forceCompile" : "false"
    },
    "notificator": {
        "url"   : "https://xns.xeaf.net",
        "key"   : "00000000-0000-0000-0000-000000000000",
        "enable": false
    }
}
